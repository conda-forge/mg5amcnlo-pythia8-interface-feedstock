set crash_on_error True

generate p p > t t~

display particles
display multiparticles

add process p p > W+ j, W+ > l+ vl @2

output PYTHIA8_INTERFACE_MG5_RUN

launch PYTHIA8_INTERFACE_MG5_RUN
  # Use PYTHIA8 for the shower/hadronization
  shower=Pythia8
  # Generate 1000 events
  set nevents 1000
  # Set random seed
  set iseed 0
